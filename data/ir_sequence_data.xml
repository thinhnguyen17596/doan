<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        
        <record id="motel_my_invoice_code_sequence" model="ir.sequence">
            <field name="name">Sequence my invoice</field>
            <field name="code">my.invoice</field>
            <field name="prefix">INV%(year)s-%(month)s/</field>
            <field name="padding">3</field>
        </record>
        
        <record id="motel_invoice_detail_code_sequence" model="ir.sequence">
            <field name="name">Sequence invoice detail</field>
            <field name="code">invoice.detail</field>
            <field name="prefix">INVD%(year)s-%(month)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="motel_report_code_sequence" model="ir.sequence">
            <field name="name">Report</field>
            <field name="code">report</field>
            <field name="prefix">Report/</field>
            <field name="padding">3</field>
        </record>
        
        
    </data>
</odoo>
